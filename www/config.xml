<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://fynkart.com" id="com.fynkart" versionCode="10" version="1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>Fynkart</name>

    <description>
        Online shopping
    </description>

    <author href="https://build.phonegap.com" email="savenzersoftech@gmail.com">
        Savenzer Softech
    </author>

    <access origin="*" />

    <platform name="android" />
    <icon src="img/logo.png" gap:role="default" />
    
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#006448" />

    <plugin name="cordova-plugin-whitelist" source="npm" />
</widget>
